---
pageable: true
per_page: 10
---

<div class="page-header">
  <h1><%= tagname %></h1>
</div>


  <% page_articles.each_with_index do |article, i| %>
    <h2><%= link_to article.title, article %> <span><%= article.date.strftime('%b %e') %></span></h2>
    <!--
    <%= article.body %>
    -->
  <% end %>


<% if paginate %>
<nav>
  <ul class="pager">
<% end %>

  <% if paginate && num_pages > 1 %>
    <!-- <p>Page <%= page_number %> of <%= num_pages %></p> -->
    <% if prev_page %>
      <li class="previous"><%= link_to 'Previous page', prev_page %></li>
    <% end %>
  <% end %>

  <% if paginate %>
    <% if next_page %>
      <li class="next"><%= link_to 'Next page', next_page %></li>
    <% end %>
  <% end %>

<% if paginate %>
  </ul>
</nav>
<% end %>
